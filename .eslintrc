{
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "strict": "off",
    "react/prop-types": "off",
    "react/no-danger": "off"
  },
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["emotion"]
}
